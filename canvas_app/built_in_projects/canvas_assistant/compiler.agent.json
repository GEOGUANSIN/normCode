{
  "description": "Agent configuration for the NormCode Compiler project",
  "default_agent": "compiler",
  
  "agents": [
    {
      "id": "compiler",
      "name": "Compiler Agent",
      "description": "Main agent for the NormCode compiler - handles chat interactions and canvas commands",
      "tools": {
        "llm": {
          "model": "qwen-plus"
        },
        "paradigm": {
          "dir": "provisions/paradigms"
        },
        "file_system": {
          "enabled": true,
          "base_dir": null
        },
        "python_interpreter": {
          "enabled": false,
          "timeout": 30
        },
        "user_input": {
          "enabled": true,
          "mode": "async"
        }
      }
    },
    {
      "id": "classifier",
      "name": "Command Classifier",
      "description": "Lightweight agent for classifying user commands - uses faster model",
      "tools": {
        "llm": {
          "model": "qwen-turbo-latest"
        },
        "file_system": {
          "enabled": false
        },
        "python_interpreter": {
          "enabled": false
        },
        "user_input": {
          "enabled": false,
          "mode": "disabled"
        }
      }
    }
  ],
  
  "mappings": [
    {
      "match_type": "flow_index",
      "pattern": "1\\.2.*",
      "agent_id": "classifier",
      "priority": 10
    }
  ],
  
  "llm_providers": [
    {
      "provider_name": "qwen-plus",
      "provider_type": "dashscope",
      "model": "qwen-plus",
      "api_key": "${DASHSCOPE_API_KEY}"
    },
    {
      "provider_name": "qwen-turbo",
      "provider_type": "dashscope", 
      "model": "qwen-turbo-latest",
      "api_key": "${DASHSCOPE_API_KEY}"
    }
  ],
  
  "created_at": "2025-12-28T12:00:00Z",
  "updated_at": "2026-01-18T16:10:00Z"
}
