# NormCode Canvas - LLM Configuration Template
# ============================================
#
# Copy this file to the PROJECT ROOT as 'settings.yaml' and fill in your API keys.
# The canvas app reads from: {project_root}/settings.yaml
#
# Format:
#   model-name:
#       API_KEY_NAME: your-api-key-here
#
# The model name becomes available in the Settings Panel dropdown.
# 'demo' mode is always available (no LLM calls, for testing).

# ============================================
# DashScope (Alibaba Cloud - Qwen models)
# ============================================
# Get your key at: https://dashscope.console.aliyun.com/

qwen-plus:
    DASHSCOPE_API_KEY: sk-your-dashscope-key-here

qwen-turbo-latest:
    DASHSCOPE_API_KEY: sk-your-dashscope-key-here

qwen-max:
    DASHSCOPE_API_KEY: sk-your-dashscope-key-here

deepseek-r1-distill-qwen-1.5b:
    DASHSCOPE_API_KEY: sk-your-dashscope-key-here

