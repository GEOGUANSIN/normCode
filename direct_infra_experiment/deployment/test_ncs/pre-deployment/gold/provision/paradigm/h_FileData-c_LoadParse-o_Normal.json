{
  "metadata": {
    "description": "Loads a file from a file_location path, parses it as JSON, and returns the data wrapped in Normal format.",
    "inputs": {
      "vertical": {},
      "horizontal": {
        "input_1": "The file_location perceptual sign (path to JSON file)."
      }
    },
    "outputs": {
      "type": "Normal",
      "description": "The parsed JSON data as a dict."
    }
  },
  "env_spec": {
    "tools": [
      {
        "tool_name": "composition_tool",
        "affordances": [
          {
            "affordance_name": "compose",
            "call_code": "result = tool.compose(plan=params['plan'], return_key=params.get('return_key'))"
          }
        ]
      },
      {
        "tool_name": "formatter_tool",
        "affordances": [
          {
            "affordance_name": "get",
            "call_code": "result = tool.get"
          },
          {
            "affordance_name": "wrap",
            "call_code": "result = tool.wrap"
          },
          {
            "affordance_name": "parse_json",
            "call_code": "import json\nresult = json.loads"
          }
        ]
      },
      {
        "tool_name": "file_system",
        "affordances": [
          {
            "affordance_name": "read",
            "call_code": "result = tool.read"
          }
        ]
      }
    ]
  },
  "sequence_spec": {
    "steps": [
      {
        "step_index": 1,
        "affordance": "formatter_tool.get",
        "params": {},
        "result_key": "dict_get_fn"
      },
      {
        "step_index": 2,
        "affordance": "file_system.read",
        "params": {},
        "result_key": "read_fn"
      },
      {
        "step_index": 3,
        "affordance": "formatter_tool.parse_json",
        "params": {},
        "result_key": "parse_json_fn"
      },
      {
        "step_index": 4,
        "affordance": "formatter_tool.wrap",
        "params": {},
        "result_key": "wrap_fn"
      },
      {
        "step_index": 5,
        "affordance": "composition_tool.compose",
        "params": {
          "plan": [
            {
              "output_key": "file_location",
              "function": {
                "__type__": "MetaValue",
                "key": "dict_get_fn"
              },
              "params": {
                "dictionary": "__initial_input__"
              },
              "literal_params": {
                "key": "input_1"
              }
            },
            {
              "output_key": "read_result",
              "function": {
                "__type__": "MetaValue",
                "key": "read_fn"
              },
              "params": {
                "__positional__": "file_location"
              }
            },
            {
              "output_key": "file_content",
              "function": {
                "__type__": "MetaValue",
                "key": "dict_get_fn"
              },
              "params": {
                "dictionary": "read_result"
              },
              "literal_params": {
                "key": "content"
              }
            },
            {
              "output_key": "parsed_data",
              "function": {
                "__type__": "MetaValue",
                "key": "parse_json_fn"
              },
              "params": {
                "__positional__": "file_content"
              }
            },
            {
              "output_key": "wrapped_result",
              "function": {
                "__type__": "MetaValue",
                "key": "wrap_fn"
              },
              "params": {
                "__positional__": "parsed_data"
              }
            }
          ],
          "return_key": "wrapped_result"
        },
        "result_key": "instruction_fn"
      }
    ]
  }
