{
  "metadata": {
    "description": "Passes through input data unchanged, wrapped in Normal format. Used for pure assertions where TIA handles all logic.",
    "inputs": {
      "vertical": {},
      "horizontal": {
        "input_1": "The data to pass through (already in-memory)."
      }
    },
    "outputs": {
      "type": "Normal",
      "description": "The same data, wrapped in Normal format."
    }
  },
  "env_spec": {
    "tools": [
      {
        "tool_name": "composition_tool",
        "affordances": [
          {
            "affordance_name": "compose",
            "call_code": "result = tool.compose(plan=params['plan'], return_key=params.get('return_key'))"
          }
        ]
      },
      {
        "tool_name": "formatter_tool",
        "affordances": [
          {
            "affordance_name": "get",
            "call_code": "result = tool.get"
          },
          {
            "affordance_name": "wrap",
            "call_code": "result = tool.wrap"
          }
        ]
      }
    ]
  },
  "sequence_spec": {
    "steps": [
      {
        "step_index": 1,
        "affordance": "formatter_tool.get",
        "params": {},
        "result_key": "dict_get_fn"
      },
      {
        "step_index": 2,
        "affordance": "formatter_tool.wrap",
        "params": {},
        "result_key": "wrap_fn"
      },
      {
        "step_index": 3,
        "affordance": "composition_tool.compose",
        "params": {
          "plan": [
            {
              "output_key": "input_data",
              "function": {
                "__type__": "MetaValue",
                "key": "dict_get_fn"
              },
              "params": {
                "dictionary": "__initial_input__"
              },
              "literal_params": {
                "key": "input_1"
              }
            },
            {
              "output_key": "wrapped_result",
              "function": {
                "__type__": "MetaValue",
                "key": "wrap_fn"
              },
              "params": {
                "__positional__": "input_data"
              }
            }
          ],
          "return_key": "wrapped_result"
        },
        "result_key": "instruction_fn"
      }
    ]
  }
}

