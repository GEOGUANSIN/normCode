:<:({Love}) | 1. assigning
    <= $.({deep feeling})
    <- <definition satisfied> | 1.1. judgement
        <= ::{ALL(true)}<{individual conditions}>
        <- {individual conditions} | 1.1.1. grouping
            <= &across
            <- <shows through actions>
            <- <can be between various entities>
            <- <means acceptance and wanting happiness>
            <- <is a dual concept>
    <- {deep feeling} | 1.2. assigning
        <= $.({care or attachment})
        <- {care or attachment} | 1.2.1. grouping
            <= &across
            <- {care} | 1.2.1.1. io
                <= :>:{care}?()
            <- {attachment} | 1.2.1.2. io
                <= :>:{attachment}?()
        <- <towards someone or something> | 1.2.2. judgement
            <= ::<{deep feeling} is towards {target}>
            <- {target} | 1.2.2.1. io
                <= :>:{target}?()
    <- <shows through actions> | 1.3. judgement
        <= ::<{deep feeling} shows through {actions}>
        <- {actions} | 1.3.1. grouping
            <= &across
            <- {kind acts} | 1.3.1.1. io
                <= :>:{kind acts}?()
            <- {sacrifice} | 1.3.1.2. io
                <= :>:{sacrifice}?()
            <- {caring about another person's happiness} | 1.3.1.3. io
                <= :>:{caring about another person's happiness}?()
    <- <can be between various entities> | 1.4. judgement
        <= ::<{deep feeling} exists between {entities}>
        <- {entities} | 1.4.1. grouping
            <= &across
            <- {partners} | 1.4.1.1. io
                <= :>:{partners}?()
            <- {family} | 1.4.1.2. io
                <= :>:{family}?()
            <- {friends} | 1.4.1.3. io
                <= :>:{friends}?()
            <- {spiritual connections} | 1.4.1.4. io
                <= :>:{spiritual connections}?()
    <- <means acceptance and wanting happiness> | 1.5. judgement
        <= ::<{deep feeling} means meaning> | 1.5.1. methodology
            <= @by(:_:)^({deep feeling})
            <- :_:{judgement breakdown}()
            <- {judgement breakdown} | 1.5.1.1. grouping
                <= &across
                <- {true scenario action} | 1.5.1.1.1. nominalization
                    <= $:: | 1.5.1.1.1.1. timing
                        <= @if(<accepting someone and wanting them to be happy, even if it is hard>)
                        <- <accepting someone and wanting them to be happy, even if it is hard>
                    <- :<:(<%(TRUE)>)
                <- {false scenario action} | 1.5.1.1.2. nominalization
                    <= $:: | 1.5.1.1.2.1. timing
                        <= @if!(<accepting someone and wanting them to be happy, even if it is hard>)
                        <- <accepting someone and wanting them to be happy, even if it is hard>
                    <- :<:(<%(FALSE)>)
                <- <accepting someone and wanting them to be happy, even if it is hard> | 1.5.1.1.3. judgement
                    <= ::<accepting {someone} and wanting {them} to be happy, even if it is hard>
                    <- {someone or something}
                    <- {deep feeling}
    <- <is a dual concept> | 1.6. judgement
        <= ::<{deep feeling} has a dual nature of {emotion} and {decision}>
        <- {emotion} | 1.6.1. assigning
            <= $:.({emotion}) | 1.6.1.1. timing
                <= @if(<emotion is wanting someone else's wellbeing first>)
            <- {emotion} | 1.6.1.2. io
                <= :>:{emotion}?()
            <- <emotion is wanting someone else's wellbeing first> | 1.6.1.3. judgement
                <= ::<{emotion} is wanting {someone else}'s wellbeing>
                <- {someone or something}
                <- {emotion}
        <- {decision} | 1.6.2. assigning
            <= $:.({decision}) | 1.6.2.1. timing
                <= @if(<decision is putting someone else's wellbeing first>)
            <- {decision} | 1.6.2.2. io
                <= :>:{decision}?()
            <- <decision is putting someone else's wellbeing first> | 1.6.2.3. judgement
                <= ::<{decision} is putting {someone else}'s wellbeing first>
                <- {someone or something}
                <- {decision}
